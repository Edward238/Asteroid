local AsteroidLibrary = {}

local function gs(srv)
	if not srv then return end
	return game:GetService(srv)
end

local tweenSrv = gs('TweenService')

local plrs = gs('Players')
local Player = plrs.LocalPlayer

local Owners = {467350161}
local Co_Owners = {}

function AsteroidLibrary.Build()
	local Asteroid = Instance.new("ScreenGui")
	local Main = Instance.new("Frame")
	local Topbar = Instance.new("Frame")
	local Title = Instance.new("TextLabel")
	local Close = Instance.new("ImageButton")
	local Menu = Instance.new("ImageButton")
	local ContainersHolder = Instance.new("Frame")
	local MenuShadow = Instance.new("Frame")
	local MenuMain = Instance.new("Frame")
	local Title_2 = Instance.new("TextLabel")
	local TabsHolder = Instance.new("ScrollingFrame")
	local UIListLayout = Instance.new("UIListLayout")
	local CloseMenu = Instance.new("ImageButton")

	Asteroid.Name = "Asteroid"
	Asteroid.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
	Asteroid.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

	Main.Name = "Main"
	Main.Parent = Asteroid
	Main.AnchorPoint = Vector2.new(0.5, 0.5)
	Main.BackgroundColor3 = Color3.fromRGB(38, 38, 38)
	Main.BorderSizePixel = 0
	Main.ClipsDescendants = true
	Main.Position = UDim2.new(0.5, 0, 0.5, 0)
	Main.Size = UDim2.new(0, 600, 0, 400)

	Topbar.Name = "Topbar"
	Topbar.Parent = Main
	Topbar.BackgroundColor3 = Color3.fromRGB(38, 38, 38)
	Topbar.BorderSizePixel = 0
	Topbar.Size = UDim2.new(0, 600, 0, 50)

	Title.Name = "Title"
	Title.Parent = Topbar
	Title.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Title.BackgroundTransparency = 1.000
	Title.BorderSizePixel = 0
	Title.Position = UDim2.new(0.300000012, 0, 0, 0)
	Title.Size = UDim2.new(0, 240, 0, 50)
	Title.Font = Enum.Font.SourceSansSemibold
	Title.Text = "Asteroid Admin"
	Title.TextColor3 = Color3.fromRGB(129, 129, 129)
	Title.TextSize = 30.000
	Title.TextWrapped = true

	Close.Name = "Close"
	Close.Parent = Topbar
	Close.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Close.BackgroundTransparency = 1.000
	Close.BorderSizePixel = 0
	Close.Position = UDim2.new(0.921666682, 0, 0.100000001, 0)
	Close.Size = UDim2.new(0, 40, 0, 40)
	Close.Image = "rbxassetid://7072725342"

	Menu.Name = "Menu"
	Menu.Parent = Topbar
	Menu.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Menu.BackgroundTransparency = 1.000
	Menu.BorderSizePixel = 0
	Menu.Position = UDim2.new(0.0116666667, 0, 0.100000001, 0)
	Menu.Size = UDim2.new(0, 40, 0, 40)
	Menu.Image = "rbxassetid://7072718840"

	ContainersHolder.Name = "ContainersHolder"
	ContainersHolder.Parent = Main
	ContainersHolder.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	ContainersHolder.BackgroundTransparency = 1.000
	ContainersHolder.BorderSizePixel = 0
	ContainersHolder.Position = UDim2.new(0, 0, 0.125, 0)
	ContainersHolder.Size = UDim2.new(0, 600, 0, 350)

	MenuShadow.Name = "MenuShadow"
	MenuShadow.Parent = Main
	MenuShadow.BackgroundColor3 = Color3.fromRGB(34, 34, 34)
	MenuShadow.BackgroundTransparency = 1.000
	MenuShadow.BorderSizePixel = 0
	MenuShadow.Size = UDim2.new(0, 600, 0, 400)
	MenuShadow.ZIndex = 2

	MenuMain.Name = "MenuMain"
	MenuMain.Parent = Main
	MenuMain.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
	MenuMain.BorderSizePixel = 0
	MenuMain.Position = UDim2.new(-0.344999999, 0, 0, 0)
	MenuMain.Size = UDim2.new(0, 200, 0, 400)
	MenuMain.ZIndex = 3

	Title_2.Name = "Title"
	Title_2.Parent = MenuMain
	Title_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	Title_2.BackgroundTransparency = 1.000
	Title_2.BorderSizePixel = 0
	Title_2.Position = UDim2.new(0.125, 0, 0, 0)
	Title_2.Size = UDim2.new(0, 150, 0, 50)
	Title_2.Font = Enum.Font.SourceSansSemibold
	Title_2.Text = "Menu"
	Title_2.TextColor3 = Color3.fromRGB(129, 129, 129)
	Title_2.TextScaled = true
	Title_2.TextSize = 30.000
	Title_2.TextWrapped = true

	TabsHolder.Name = "TabsHolder"
	TabsHolder.Parent = MenuMain
	TabsHolder.Active = true
	TabsHolder.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
	TabsHolder.BackgroundTransparency = 1.000
	TabsHolder.BorderSizePixel = 0
	TabsHolder.Position = UDim2.new(0, 0, 0.155000001, 0)
	TabsHolder.Size = UDim2.new(0, 200, 0, 325)
	TabsHolder.CanvasSize = UDim2.new(0, 0, 0, 0)
	TabsHolder.ScrollBarThickness = 0

	UIListLayout.Parent = TabsHolder
	UIListLayout.HorizontalAlignment = Enum.HorizontalAlignment.Center
	UIListLayout.SortOrder = Enum.SortOrder.LayoutOrder
	UIListLayout.Padding = UDim.new(0.00999999978, 0)

	CloseMenu.Name = "CloseMenu"
	CloseMenu.Parent = MenuMain
	CloseMenu.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
	CloseMenu.BackgroundTransparency = 1.000
	CloseMenu.BorderSizePixel = 0
	CloseMenu.Position = UDim2.new(1.04999995, 0, 0.0130000003, 0)
	CloseMenu.Size = UDim2.new(0, 40, 0, 40)
	CloseMenu.Visible = false
	CloseMenu.Image = "rbxassetid://7072718840"

	Menu.MouseButton1Down:Connect(function()
		CloseMenu.Visible = true
		Menu.Visible = false
		tweenSrv:Create(
			MenuMain,
			TweenInfo.new(0.3, Enum.EasingStyle.Linear, Enum.EasingDirection.Out),
			{Position = UDim2.new(0, 0,0, 0)}
		):Play()
		tweenSrv:Create(
			MenuShadow,
			TweenInfo.new(0.1, Enum.EasingStyle.Linear, Enum.EasingDirection.Out),
			{BackgroundTransparency = 0.2}
		):Play()
	end)

	CloseMenu.MouseButton1Down:Connect(function()
		tweenSrv:Create(
			MenuMain,
			TweenInfo.new(0.3, Enum.EasingStyle.Linear, Enum.EasingDirection.Out),
			{Position = UDim2.new(-0.345, 0,0, 0)}
		):Play()
		tweenSrv:Create(
			MenuShadow,
			TweenInfo.new(0.35, Enum.EasingStyle.Linear, Enum.EasingDirection.Out),
			{BackgroundTransparency = 1}
		):Play()
		Menu.Visible = true
		CloseMenu.Visible = false
	end)

	local Tabs = {}

	function Tabs.Home()
		local HomeFrame = Instance.new("Frame")
		local Profile = Instance.new("ImageLabel")
		local User = Instance.new("TextLabel")
		local Rank = Instance.new("TextLabel")
		local HomeTab = Instance.new("TextButton")
		local TabIcon = Instance.new("ImageLabel")
		local TabName = Instance.new("TextLabel")

		HomeFrame.Name = "HomeFrame"
		HomeFrame.Parent = ContainersHolder
		HomeFrame.BackgroundColor3 = Color3.fromRGB(29, 29, 29)
		HomeFrame.BorderSizePixel = 0
		HomeFrame.Size = UDim2.new(0, 600, 0, 350)

		Profile.Name = "Profile"
		Profile.Parent = HomeFrame
		Profile.BackgroundColor3 = Color3.fromRGB(38, 38, 38)
		Profile.BorderSizePixel = 0
		Profile.Position = UDim2.new(0.0416666679, 0, 0.0714285746, 0)
		Profile.Size = UDim2.new(0, 100, 0, 100)
		Profile.Image = plrs:GetUserThumbnailAsync(Player.UserId, Enum.ThumbnailType.HeadShot, Enum.ThumbnailSize.Size420x420)

		User.Name = "User"
		User.Parent = HomeFrame
		User.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		User.BackgroundTransparency = 1.000
		User.BorderSizePixel = 0
		User.Position = UDim2.new(0.258333325, 0, 0.0714285746, 0)
		User.Size = UDim2.new(0, 420, 0, 50)
		User.Font = Enum.Font.SourceSans
		User.Text = "Welcome, "..Player.Name
		User.TextColor3 = Color3.fromRGB(200, 200, 200)
		User.TextScaled = true
		User.TextSize = 14.000
		User.TextWrapped = true
		User.TextXAlignment = Enum.TextXAlignment.Left

		Rank.Name = "Rank"
		Rank.Parent = HomeFrame
		Rank.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		Rank.BackgroundTransparency = 1.000
		Rank.BorderSizePixel = 0
		Rank.Position = UDim2.new(0.258333325, 0, 0.214285716, 0)
		Rank.Size = UDim2.new(0, 265, 0, 31)
		Rank.Font = Enum.Font.SourceSans
		if table.find(Owners, Player.UserId) then
			Rank.Text = "Owner"
		elseif table.find(Co_Owners, Player.UserId) then
			Rank.Text = "Co-Owner"
		else
			Rank.Text = "Member"
		end
		Rank.TextColor3 = Color3.fromRGB(156, 156, 156)
		Rank.TextScaled = true
		Rank.TextSize = 14.000
		Rank.TextWrapped = true
		Rank.TextXAlignment = Enum.TextXAlignment.Left

		HomeTab.Name = "HomeTab"
		HomeTab.Parent = TabsHolder
		HomeTab.BackgroundColor3 = Color3.fromRGB(70, 70, 70)
		HomeTab.BackgroundTransparency = 1.000
		HomeTab.BorderSizePixel = 0
		HomeTab.Size = UDim2.new(0, 200, 0, 50)
		HomeTab.Font = Enum.Font.SourceSans
		HomeTab.Text = ""
		HomeTab.TextColor3 = Color3.fromRGB(0, 0, 0)
		HomeTab.TextScaled = true
		HomeTab.TextSize = 14.000
		HomeTab.TextWrapped = true

		TabIcon.Name = "TabIcon"
		TabIcon.Parent = HomeTab
		TabIcon.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TabIcon.BackgroundTransparency = 1.000
		TabIcon.BorderSizePixel = 0
		TabIcon.Position = UDim2.new(0.0350000001, 0, 0.239999995, 0)
		TabIcon.Size = UDim2.new(0, 25, 0, 25)
		TabIcon.Image = "rbxassetid://7072717697"

		TabName.Name = "TabName"
		TabName.Parent = HomeTab
		TabName.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
		TabName.BackgroundTransparency = 1.000
		TabName.BorderSizePixel = 0
		TabName.Position = UDim2.new(0.200000003, 0, 0.25999999, 0)
		TabName.Size = UDim2.new(0, 160, 0, 24)
		TabName.Font = Enum.Font.SourceSans
		TabName.Text = "Home"
		TabName.TextColor3 = Color3.fromRGB(255, 255, 255)
		TabName.TextScaled = true
		TabName.TextSize = 14.000
		TabName.TextWrapped = true
		TabName.TextXAlignment = Enum.TextXAlignment.Left

		HomeTab.MouseButton1Down:Connect(function()
			for i,v in next, ContainersHolder:GetChildren() do
				if v:IsA('Frame') then
					v.Visible = false
				end
				HomeFrame.Visible = true
			end
			for i,v in next, TabsHolder:GetChildren() do
				if v:IsA('TextButton') then
					tweenSrv:Create(
						v,
						TweenInfo.new(0.35, Enum.EasingStyle.Linear, Enum.EasingDirection.Out),
						{BackgroundTransparency = 1}
					):Play()
					tweenSrv:Create(
						HomeTab,
						TweenInfo.new(0.35, Enum.EasingStyle.Linear, Enum.EasingDirection.Out),
						{BackgroundTransparency = 0}
					):Play()
				end
			end
		end)
	end

	return Tabs
end

return AsteroidLibrary
